{
  "$schema": "./node_modules/@openapitools/openapi-generator-cli/config.schema.json",
  "spaces": 2,
  "generator-cli": {
    "version": "7.6.0",
    "generators": {
      "v1.0": {
        "generatorName": "typescript-rxjs",
        "output": "./src/api/rxjs-client",
        "skipValidateSpec": true,
        "inputSpec": "https://raw.githubusercontent.com/RobotsAndPencils/AkrochemERP/main/backend/docs/api.yml",
        "additionalProperties": {
          "supportsES6": true,
          "useSingleRequestParameter": true
        }
      }
    }
  }
}
