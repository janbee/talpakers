{
  "name": "play-ab-web",
  "version": "0.0.2",
  "homepage": "https://janbee.github.io/talpakers",
  "private": true,
  "scripts": {
    "i": "npm install",
    "rm:husky": "rm -rf .husky ",
    "prepare:husky:precommit": "npm run rm:husky && .husky/pre-commit \"npm --no-git-tag-version version patch && git add . && npm run deploy\"",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "git:add": "git add .",
    "git:commit": "git commit -a -m 'auto'",
    "git:push": "git push",
    "git:run": "npm run git:add && npm run git:commit && npm run git:push",
    "build": "tsc && vite build",
    "prepare": "husky"
  },
  "dependencies": {
    "gh-pages": "^6.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo gagagag"
    }
  },
  "lint-staged": {
    "*.{jsx,ts,tsx,css,md,json}": [
      "git add .",
      "prettier --write './**/*.{ts,tsx}'"
    ]
  },
  "devDependencies": {
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10"
  }
}
