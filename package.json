{
  "name": "play-ab-web",
  "version": "0.1.145",
  "homepage": "https://janbee.github.io/talpakers/build",
  "private": true,
  "scripts": {
    "rm": "rm -rf node_modules && rm yarn.lock || rd /s /q node_modules",
    "yrn:update": "yarn upgrade-interactive --latest",
    "i": "yarn install",
    "prettier:format": "prettier --write './**/*.{jsx,ts,tsx,css,md,json}'  --config ./.prettierrc.js",
    "prepare:husky": "rm -rf .husky & npx husky install && npx husky add .husky/pre-commit \"npm --no-git-tag-version version patch && git add . && yarn build && npx --no-install lint-staged\"",
    "------------------ ----------------------- ------------------": "",
    "start": "craco start --open",
    "build": "craco build && cp ./build/index.html ./",
    "test": "craco test",
    "------------------ git ------------------": "",
    "git:add": "git add .",
    "git:commit": "git commit -a -m 'auto'",
    "git:push": "git push",
    "git:run": "yarn run git:add && yarn run git:commit && yarn run git:push"
  },
  "dependencies": {
    "@craco/craco": "^7.1.0",
    "@realm/babel-plugin": "^0.1.1",
    "@testing-library/jest-dom": "^6.1.4",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.5",
    "@types/lodash": "^4.14.184",
    "@types/moment": "^2.13.0",
    "@types/node": "^20.8.6",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "bson": "^6.2.0",
    "classnames": "^2.5.1",
    "crypto-browserify": "^3.12.0",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.12",
    "react-router-dom": "6.21.2",
    "react-scripts": "^5.0.1",
    "realm-web": "^2.0.0",
    "rxjs": "^7.5.6",
    "sass": "^1.69.7",
    "stream": "^0.0.2",
    "stream-browserify": "^3.0.0",
    "ts-jest": "^29.1.1",
    "use-font-face-observer": "^1.2.1",
    "web-vitals": "^3.5.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@typescript-eslint/eslint-plugin": "^5.33.0",
    "@typescript-eslint/parser": "^5.33.0",
    "core-js-pure": "^3.23.3",
    "craco-alias": "^3.0.1",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "^8.0.1",
    "lint-staged": "^14.0.1",
    "pre-commit": "^1.2.2",
    "prettier": "^3.0.3",
    "semantic-ui-react": "^2.1.5",
    "typescript": "5.1.6"
  },
  "lint-staged": {
    "*.{jsx,ts,tsx,css,md,json}": [
      "git add .",
      "prettier --write './**/*.{ts,tsx}'"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
