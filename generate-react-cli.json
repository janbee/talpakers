{
  "usesTypeScript": true,
  "usesStyledComponents": false,
  "usesCssModule": false,
  "cssPreprocessor": "css",
  "testLibrary": "Testing Library",
  "component": {
    "default": {
      "path": "src/domains",
      "withStyle": false,
      "withTest": true,
      "withStory": false,
      "withLazy": false,
      "customDirectory": "template-name",
      "customTemplates": {
        "component": "generate-react-templates/domains/TemplateName.tsx",
        "test": "generate-react-templates/domains/TemplateName.test.tsx"
      }
    },
    "page": {
      "path": "src/pages",
      "withStyle": false,
      "withTest": false,
      "withStory": false,
      "withLazy": false,
      "customDirectory": "template-name",
      "customTemplates": {
        "component": "generate-react-templates/pages/index.tsx"
      }
    },
    "common": {
      "path": "src/common",
      "withStyle": false,
      "withTest": true,
      "withStory": false,
      "withLazy": false,
      "customDirectory": "template-name",
      "customTemplates": {
        "component": "generate-react-templates/common/TemplateName.tsx"
      }
    },

    "hook": {
      "path": "src/domains",
      "withStyle": false,
      "withTest": true,
      "withStory": false,
      "withLazy": false,
      "flat": true,
      "customTemplates": {
        "component": "generate-react-templates/hooks/TemplateName.tsx"
      }
    }
  }
}
